    <script src="/../admin/js/demo.js"></script>

    <script type="text/javascript">
        $("button[name=btnUpdate]").on("click", function(e) {
            e.preventDefault();
            $('#contactsForm').attr('action', "/test1").submit();
        });
    </script>

